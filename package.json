{
  "name": "nuxt-apollo-hasura",
  "organization": "NLeSC eScienceCenter",
  "version": "1.1.0",
  "private": true,
  "description": "Nuxt Apollo Hasura Postgres Template",
  "author": "Jesus Garcia Gonzalez",
  "scripts": {
    "serve": "docker-compose up -d && yarn dev",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "deploy": "yarn build:gh-pages && GIT_DEPLOY_REPO=https://github.com/NLeSC/nuxt-apollo-hasura scripts/deploy.sh",
    "analyze": "yarn nuxt build --analyze",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate": "nuxt generate",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "migrate": "cd hasura && hasura migrate create hasurafiles",
    "release": "standard-version",
    "start": "nuxt start"
  },
  "dependencies": {
    "@mdi/js": "^5.3.45",
    "@nuxtjs/apollo": "^4.0.1-rc.1",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "axios": "^0.19.2",
    "cross-env": "^7.0.2",
    "jose": "^1.27.1",
    "lodash.debounce": "^4.0.8",
    "nuxt": "^2.13.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.10.3",
    "@babel/runtime-corejs3": "^7.10.3",
    "@nuxtjs/date-fns": "^1.3.0",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": ">=1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "hasura-cli": "^1.2.2",
    "keypair": "^1.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0"
  }
}
